        <div class="this_msg" style="text-align: center;width:293px;">
            <dl style="width:100%">
                <dt>收藏成功！</dt>
                <dt>可前往美食林首页-顶部-我的收藏查看</dt>
            </dl>
        </div>
    </section>

    </div>
    </div>
</script>
<script id="commentTmpl" type="text/lizard-template">
    <section class="mdetail_comment" style="margin-top:8px;">
        <!--点评改个新标题-->
        <div class="c-ratedtitle heightauto">
            <div class="c-ratedtitle__item">
                <h3>游友点评</h3>
            </div>
            <%if(CommentCount > 2){%>
            <div class="c-ratedtitle__rlink js_more_comment">查看全部</div>
            <%}%>
        </div>
        <div class="c-tt-title__title--comment">
            <div class="ui-comment-box ui-comment-star<%=commonUtil.getStar(CommentResult.AverageScore)%>">
                <i class="common-iconfont ui-comment-star"></i>
                <i class="common-iconfont ui-comment-star"></i>
                <i class="common-iconfont ui-comment-star"></i>
                <i class="common-iconfont ui-comment-star"></i>
                <i class="common-iconfont ui-comment-star"></i>
            </div>
            <strong><%= CommentResult.AverageScore %>分</strong><span class="comment_count"><%=CommentCount%>条点评</span>
        </div>

        <% if (CommentCount > 50 && CommentResult.CommentHotTags && CommentResult.CommentHotTags.length > 0 && !commonUtil.isGsHybrid()) { %>
        <div class="tag-list">
            <ul class="js_key_tags">
                <% _.each(CommentResult.CommentHotTags,function(value,index){%>
                <%if(index <= 8 && value.CommentCount>0){ %>
                <%if(!(value.TagContent=='最新' || value.TagContent=='好评' || value.TagContent=='差评' || value.TagContent=='带图'|| value.TagContent=='来自订单' || value.TagContent=='来自游记')){%>
                <li data-tagId="<%=value.TagId %>" id="c_tag_comment" data-index="<%=index %>"><%=value.TagContent %><span><%=value.CommentCount %></span></li>
                <%}%>
                <%}%>
                <%})%>
            </ul>
        </div>
        <%}%>
        <ul>
            <% _.each(commonUtil.limitTo(CommentList,3),function(v,k){ %>
            <li class="comment_block js_tag">
                <!-- 7.6公共头 -->
                <div class="user-info">
                    <div class="user-info__portrait js_user" id="c_spotdetail_comment_user" data-auth="<%=v.UserInfoModel.ClientAuth%>" data-source="<%=v.SourceType %>">
                        <img src="<%=v.UserInfoModel.UserImageSrc%>"/>
                    </div>
                    <div class="user-info__content">
                        <div class="user-detail">
                            <div class="user-detail__name js_author"><%=v.UserInfoModel.UserNick%></div>
                            <div class="user-detail__tags"><% if(v.UserInfoModel.UserVIPLevel > 0){%><span class="user-detail__tags--level">VIP<%=v.UserInfoModel.UserVIPLevel %></span><%}%><% _.each(v.IdentityNames,function(value,i){%><span class="user-detail__tags--title"><%=value%></span><%})%></div>
                        </div>
                        <div class="ui-comment-box ui-comment-star<%=commonUtil.getStar(v.TotalStar)%>">
                            <i class="common-iconfont ui-comment-star"></i>
                            <i class="common-iconfont ui-comment-star"></i>
                            <i class="common-iconfont ui-comment-star"></i>
                            <i class="common-iconfont ui-comment-star"></i>
                            <i class="common-iconfont ui-comment-star"></i>
                        </div>
                    </div>
                    <% if(v.IsPicked){ %>
                    <div class="user-info__epic"></div>
                    <%}else{%>
                    <div class="user-info__hold" style="display: none">待审核</div>
                    <%}%>
                </div>
                <div class="paragraph_comment js_commentP" id="c_spotdetail_single_comment<%=k%>" data-commentid="<%=v.CommentId%>">
                    <p class="plain_comment fold js_comment_desc" id="c_spotdetail_single_comment">
                        <%=commonUtil.xssFilter(v.Content)%>
                    </p>
                </div>
                <%if(v.Images&&v.Images.length>0){%>
                <div class="img-group js_comment_pic" id="c_spotdetail_comment_pic">
                    <% _.each(v.Images,function(item,t) {  %>
                    <% if (t < 4) { %>
                    <a href="javascript:void(0)" class="js_image" data-superindex="<%= k %>" data-index="<%=t%>" data-count="<%=v.Images.length %>" data-commentid="<%=v.CommentId%>" data-from="<%= v.UserInfoModel.UserNick %>">
                        <img src="<%=item.ThumbnailUrl%>" data-urllist="<%= item.BaseUrl %>">
                        <% if(v.Images.length>4 && t==3){ %>
                        <span class="count"><%=v.Images.length %>张</span>
                        <% } %>
                    </a>
                    <% } %>
                    <% }) %>
                </div>
                <%}else if(v.ShortVideoList && v.ShortVideoList.length>0){%>
                <div class="play-group" data-video="<%=v.ShortVideoList[0].PlayUrl %>">
                    <% if(Lizard.isHybrid || Lizard.isInCtripApp){%>
                    <div class="video_cover">
                        <img data-src="<%= v.ShortVideoList[0].PlayUrl%>" src="<%= v.ShortVideoList[0].CoverImageUrl %>" >
                        <div class="btn_play js_play"></div>
                    </div>
                    <%}else{%>
                    <div class="videoContainer" data-poster="<%= v.ShortVideoList[0].CoverImageUrl%>" data-src="<%= v.ShortVideoList[0].PlayUrl%>"></div>
                    <%}%>
                </div>

                <%}%>
            </li>
            <%})%>
        </ul>
        <%if(CommentCount > 2){%>
        <p href="/html5/you/comment/DistrictId/SightId-sight.html" id="delicacies_detail_commentsall" class="find_morelink more_comment">查看全部<%=CommentCount%>条点评</p>
        <%}%>

        <% if(Lizard.isHybrid || Lizard.isInCtripApp){%>
        <div class="videoContainer" style="display: none"></div>
        <%}%>
    </section>
</script>

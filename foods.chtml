@using System.Configuration;
<link rel="stylesheet" href="@ConfigurationManager.AppSettings["WebresourceBaseUrl"]build/common/swiper.css" type="text/css" />
<script type="text/lizard-config">
    {
        "url_schema": ["foods/{{cityname=/\\D*/}}{{(int)cityid}}/{restId}.html","foods/restaurant/{restId}","foods/restaurant/{restId}.html","food/{{cityname=/\\D*/}}{{(int)cityid}}/{restId}.html", "foods/{{fromHotel=/hotelFoodsDetail/}}/{{cityname=/\\D*/}}{{(int)cityid}}/{restId}.html"],
        "viewName": commonUtil.genViewName(),
        "model": commonUtil.genModel({
            "apis":[
                {
                url:'10332/GetRestaurantInfoV705',
                postdata:{
                    "RestaurantId": Lizard.P('restId'),
                    "ImageQuality": (commonUtil.getImageQuality()||2),
                    "CurrentDestId":(commonUtil.getLocalPosition().districtId||2),
                    "Lon":(commonUtil.getLocalPosition().lng||0),
                    "Lat":(commonUtil.getLocalPosition().lat||0),
                    "Version": '7.04',
                    "Platform": commonUtil.isApple() ? 1 : 2,
                    "DistrictId":Lizard.P('cityid') && parseInt(Lizard.P('cityid').match(/\d*/)[0]),
                    "MoblePhone":window.localStorage.getItem('kou_bei_telphone') || ''

                    }
                },{
                     url:'10332/GetRestaurantInfoV705Extend',
                     postdata:{
                        "RestaurantId": Lizard.P('restId'),
                        "ImageQuality": (commonUtil.getImageQuality()||2),
                        "CurrentDestId":(commonUtil.getLocalPosition().districtId||2),
                        "Lon":(commonUtil.getLocalPosition().lng||0),
                        "Lat":(commonUtil.getLocalPosition().lat||0),
                        "Version": '7.03',
                        "Platform": commonUtil.isApple() ? 1 : 2
                     }
                }
            ],
            "filter":function (datas){
                (function(){
                    if(Lizard.isHybrid || Lizard.isInCtripApp || commonUtil.isHybridMode()){
                        var cssNode = document.createElement('link');
                        cssNode.rel = 'stylesheet';
                        cssNode.type = 'text/css';
                        cssNode.href = 'webresource/build/common/swiper.css ';
                        document.head.appendChild(cssNode);
                    }
                })();
                var rest = datas[0];
                rest = _.extend(rest,datas[1]);
                rest.isMslType = rest.RestaurantInfo.ShiMeiLinType > 0 ? true : false;
                rest.isOldVersion = rest.RestaurantInfo.ShiMeiLinType > 0 ? false :true;
                rest.RestaurantInfo.firstName = rest.RestaurantInfo.Name || rest.RestaurantInfo.SecondName;
                rest.RestaurantInfo.secondName = rest.RestaurantInfo.SecondName || '';
                rest.line_flag = !!(rest.RestaurantInfo.OpenTime ||
                                rest.RestaurantInfo.FacilityNameList && rest.RestaurantInfo.FacilityNameList.length>0 ||
                                rest.RestaurantInfo.SpecialTips ||
                                rest.RestaurantInfo.PriceDesc||
                                rest.RestaurantInfo.PaymentCardNameList && rest.RestaurantInfo.PaymentCardNameList.length>0 ||
                                rest.RestaurantInfo.WeiXin ||
                                rest.RestaurantInfo.Twitter ||
                                rest.RestaurantInfo.Facebook ||
                                rest.RestaurantInfo.WeiBo ||
                                rest.RestaurantInfo.WebSite);
                     console.log(rest.line_flag);
                if(rest.RestaurantInfo){
                    rest.RestaurantInfo.firstName = rest.RestaurantInfo.Name || rest.RestaurantInfo.SecondName;
                    rest.RestaurantInfo.secondName = rest.RestaurantInfo.SecondName || '';
                    rest.hasPlay = false;
                    rest.hasBook = false;
                    rest.hasTuan = false;
                    if(rest.ProductList && rest.ProductList.length>0){
                        _.each(rest.ProductList, function(item, index){
                            var sourceType = (item.SourceType || '').replace(/(^\s*)|(\s*$)/g, "");
                            if(sourceType && sourceType.length>0){
                                if(sourceType == "Play"){
                                    rest.hasPlay = true;
                                }else if(sourceType == "Eztable" || sourceType == "DiningCity"){
                                    rest.hasBook = true;
                                    rest.RestaurantInfo.LinkUrl = item.LinkUrl || '';
                                }else if(sourceType == 'HotelGroup'){
                                    rest.hasTuan = true;
                                }
                            }
                        });
                    }
                    rest.Comment = {};
                }
                rest.ggLat = rest.RestaurantInfo && rest.RestaurantInfo.GDCoord && rest.RestaurantInfo.GDCoord.Lat;
                rest.ggLng = rest.RestaurantInfo && rest.RestaurantInfo.GDCoord && rest.RestaurantInfo.GDCoord.Lng;
                if (rest.RestaurantInfo && rest.RestaurantInfo.GGCoord && rest.RestaurantInfo.GGCoord.MapImageUrl) {
                    rest.mapurl = rest.RestaurantInfo.GGCoord.MapImageUrl;
                } else {
                    rest.mapurl = '';
                }
                rest.location = {
                    lat : rest.RestaurantInfo && rest.RestaurantInfo.GDCoord && rest.RestaurantInfo.GDCoord.Lat,
                    lng : rest.RestaurantInfo && rest.RestaurantInfo.GDCoord && rest.RestaurantInfo.GDCoord.Lng,
                    name : rest.RestaurantInfo && rest.RestaurantInfo.Name,
                    isoversea : rest.RestaurantInfo && rest.RestaurantInfo.IsOversea
                };

                return {
                    rest:rest
                };
            },
            setTDK: function(datas) { //只在SEO中有用， 来渲染title description keyword
                var rest = datas[0];
                rest = _.extend(rest,datas[1]);
                var restaurantName = (rest && rest.RestaurantInfo)?rest.RestaurantInfo.Name:"";
                var destination = rest && rest.RestaurantInfo && rest.RestaurantInfo.DistrictName || '';
                return {
                    title:  "{city}" +restaurantName+"电话/地址/菜系/点评/营业时间【携程攻略】",
                    keywords: restaurantName+"电话,"+restaurantName+"地址,"+restaurantName+"菜系,"+restaurantName+"点评,"+restaurantName+"营业时间",
                    description: "{city}" +restaurantName+"，携程攻略社区! 收集携程游友对{city}" +restaurantName+"餐馆的各种评价、印象、点评，包含"+restaurantName+"电话、地址、菜系、人均价格、营业时间等信息。要旅行，从携程攻略开始。",
                };
            }
        }),
        "view":{
                "viewport": Lizard.T("viewportTmpl")
            },
        "controller": Lizard.webresourceBaseUrl + "js/Foods/Views/food.js"
    }
</script>
<!-- 页面主模板 -->
<script id="viewportTmpl" type="text/lizard-template">
    <div class="l-foodheadouter_detail <%if(commonUtil.isIphoneX()){%> l-foodheadouter_detail--iosx <%}else if(commonUtil.isApple()){%> l-foodheadouter_detail--ios <%}%>">
        <div class="l-foodheadouter_detail__headwrap js_transparent" style="opacity:1">
            <div class="l-customhead_detail <%if(!rest.isMslType){%>nonmsl_detail_line<%}%>">
                <div class="l-customhead_detail__left">
                    <i class="common-iconfont ui-icon-back js_goback">&#xe621</i>
                </div>
                <div class="l-customhead_detail__center">
                    <div class="ui-title"></div>
                </div>
                <% if(commonUtil.isHybridMode()){ %>
                <% if(Lizard.P('fromHotel')!='hotelFoodsDetail' && Lizard.P('fromType')!='hotel'){ %>
                <div class="l-customhead_detail__right">
                    <i class="common-iconfont ui-icon-collect js_changefavstatus">&#xe629</i>
                    <i class="common-iconfont ui-icon-share js_share">&#xe62b</i>
                </div>
                <%}%>
                <%}%>
            </div>
        </div>
        <div class="l-foodheadouter_detail__headwrap l-foodheadouter_detail__headwrap--white js_grey"style="opacity:0">
            <div class="l-customhead_detail nonmsl_detail_line">
                <div class="l-customhead_detail__left">
                    <i class="common-iconfont ui-icon-back js_goback">&#xe621</i>
                </div>
                <div class="l-customhead_detail__center">
                    <%if(commonUtil.isHybridMode()){%>
                    <div class="ui-title js_title"><%if(rest.isMslType){%><%=rest.RestaurantInfo.Name %><%}%></div>
                    <%}else{%>
                    <div class="ui-title js_title"><%=rest.RestaurantInfo.Name %></div>
                    <%}%>
                </div>
                <% if(commonUtil.isHybridMode()){ %>
                <% if(Lizard.P('fromHotel')!='hotelFoodsDetail' && Lizard.P('fromType')!='hotel'){ %>
                <div class="l-customhead_detail__right">
                    <i class="common-iconfont ui-icon-collect js_changefavstatus">&#xe629</i>
                    <i class="common-iconfont ui-icon-share js_share">&#xe62b</i>
                </div>
                <%}%>
                <%}%>
            </div>
        </div>
        <!--h5d大图模式需要的dom结构 强耦合！！-->
        <section class="appue_scale photo_top_space">
            <div class="js_close this_return"></div>
            <div class="this_title"></div>
            <ul class="this_img"></ul>
            <div class="this_intro"></div>
        </section>

        <div class="l-sightheadouter__head-srollview food_detail">
            <!-- 美食林和非美食林头部信息-->
            <%if(rest.isMslType){%>
            <section class="topmain">
                <div class="l-topheadpic__imgshadow"></div>
                <div class="bgimg <% if(Lizard.P('fromHotel')!='hotelFoodsDetail' && Lizard.P('fromType')!='hotel'){ %>jsimgbanner<%}%> jsimgbanner_wraper"></div>
                <% if(rest.RestaurantInfo.OpenStatus && rest.RestaurantInfo.OpenStatusRemark){ %>
                <div class="running-tips"></div>
                <div class="overmasktransparent">
                    <h2><%=rest.RestaurantInfo.OpenStatus%></h2>
                    <h3 class="hide_runningtips marqueebackfoward"><%=rest.RestaurantInfo.OpenStatusRemark %></h3>
                    <h4 class="marquee"><%=rest.RestaurantInfo.OpenStatusRemark %></h4>
                </div>

                <% } %>
                <%if(rest.RestaurantInfo.ImageCount>0){ %>
                <p class="camara"><span><%=rest.RestaurantInfo.ImageCount%>张</span></p>
                <%}else if(commonUtil.isHybridMode()){%>
                <p class="camara"><span>上传图片</span></p>
                <%}%>
            </section>
            <%}else{%>
            <% if(rest.RestaurantInfo.OpenStatus){ %>
            <div class="s-running-tips" <%if(commonUtil.isApple()){%>style="top:64px;"<%}%>><%=rest.RestaurantInfo.OpenStatus %></div>
        <% } %>
        <section class="topmain" <%if(commonUtil.isIphoneX()){%> style="margin-top:88px" <%}else if(commonUtil.isApple()){%> style="margin-top:64px"<%}else{%> style="margin-top:44px"<%}%>>
        <div class="l-modlist__item top_info_over">
            <div class="c-listinnerwrap">
                <div class="c-imgbox <% if(Lizard.P('fromHotel')!='hotelFoodsDetail' && Lizard.P('fromType')!='hotel'){ %>js_picclick<%}%> picclick">
                    <%if(rest.RestaurantInfo.ImageCount>0){%>
                    <span class="picnum_detail_nonmsl"><%=rest.RestaurantInfo.ImageCount%>张</span>
                    <%}else if(commonUtil.isHybridMode()){%>
                    <span class="picnum_detail_nonmsl">上传图片</span>
                    <%}%>
                    <img src="<%= rest.RestaurantInfo.CoverImageUrls[0] %>" />
                </div>
                <div class="c-textview  <% if(rest.RestaurantInfo.IsOversea && rest.RestaurantInfo.secondName && rest.RestaurantInfo.firstName != rest.RestaurantInfo.secondName) {%>c-textview--foodoversea<%}%>">
                    <div class="c-textview__title ">
                        <div class="c-textview__title--text "><span><%=rest.RestaurantInfo.firstName%></span></div>
                    </div>
                    <% if(rest.RestaurantInfo.IsOversea && rest.RestaurantInfo.secondName && rest.RestaurantInfo.firstName != rest.RestaurantInfo.secondName) {%>
                    <div class="c-textview__ename "><%=rest.RestaurantInfo.secondName%></div>
                    <%}%>
                    <% if(rest.MeiShiLinChainTypeName){ %>
                    <div class="c-textview__comment ">
                        <div class="c-textview__comment--msltip "><%=rest.MeiShiLinChainTypeName%> · 分店</div>
                    </div>
                    <% }else if(rest.RestaurantInfo.CommentScore && rest.RestaurantInfo.CommentScore>0){%>
                    <div class="c-textview__comment">
                        <div class="c-textview__comment--score"><%=rest.RestaurantInfo.CommentScore%>分</div>
                        <div class="c-textview__comment--count"><%=rest.RestaurantInfo.CommentCount%>条点评</div>
                    </div>
                    <%}%>
                    <%if(rest.RestaurantInfo.Cuisine[0] || rest.RestaurantInfo.Labels[0] || rest.RestaurantInfo.KoubeiAmount){%>
                    <div class="c-textview__tagbox">
                        <% if(rest.RestaurantInfo.Cuisine[0]){ %>
                        <i><%=rest.RestaurantInfo.Cuisine[0].Name%></i>
                        <%}%>
                        <% if(rest.RestaurantInfo.Labels[0]){ %>
                        <i><%=rest.RestaurantInfo.Labels[0].Name%></i>
                        <%}%>
                        <% if(rest.RestaurantInfo.KoubeiAmount){%>
                        <i>口碑人气<%=rest.RestaurantInfo.KoubeiAmount%></i>
                        <%}%>
                    </div>
                    <%}%>
                    <div class="c-textview__bottom">
                        <% if(parseInt(rest.RestaurantInfo.AveragePrice)>0){ %>
                        <span>人均&yen;<%=rest.RestaurantInfo.AveragePrice%></span><%if(rest.RestaurantInfo.LocalCurrency && rest.RestaurantInfo.LocalPrice){%><span><%=rest.RestaurantInfo.LocalCurrency%><%=rest.RestaurantInfo.LocalPrice%></span><%}%>
                        <%}%>
                    </div>
                </div>
            </div>
        </div>
        </section>
        <%}%>

        <div class="warper">
            <!-- 美食林餐厅-->
            <% if(rest.isMslType){%>
            <div class="res_info">
                <div class="restinfo">
                    <h3 <%if(rest.RestaurantInfo.secondName){%>class="oversea"<%}%>>
                    <strong><%=rest.RestaurantInfo.firstName%></strong>
                    <%if(rest.RestaurantInfo.secondName && rest.RestaurantInfo.firstName != rest.RestaurantInfo.secondName){%>
                    <p><%=rest.RestaurantInfo.secondName%></p>
                    <%}%>
                    </h3>
                    <div class="food_rating js_wraper_cuiname">
                        <div class="js_scroll_cuiname">
                            <%if(rest.RestaurantInfo.ShiMeiLinName){%><span class="staert"><%=rest.RestaurantInfo.ShiMeiLinName%></span><%}%><p><span class="cuilabels"><%=rest.RestaurantInfo.Cuisine[0] && rest.RestaurantInfo.Cuisine[0].Name%></span><%if(rest.RestaurantInfo.AveragePrice){%><span>人均&yen;<%=rest.RestaurantInfo.AveragePrice%></span><%}%><%if(rest.RestaurantInfo.LocalCurrency && rest.RestaurantInfo.LocalPrice){%><span class="oversea"><%= rest.RestaurantInfo.LocalCurrency%><%=rest.RestaurantInfo.LocalPrice %></span><%}%></p>
                        </div>
                    </div>
                </div>
                <% if(rest.VideoBanner && Lizard.P('fromType') !== 'external'){ %>
                <div class="tdcp js_go_video" id="c_deep_article" data-poiid="<%= Lizard.P('restId') %>" data-link="<% if(!commonUtil.isHybridMode()){ %><%=rest.VideoBanner && rest.VideoBanner.LinkUrl %> <% }else{ %> <%=rest.VideoBanner && rest.VideoBanner.HybridUrl %> <% } %>"><span>探店测评</span></div>
                <% } %>
            </div>
            <%}%>


            <!-- 旅行者推荐-->
            <!--无“评委评语”时，显示“旅行者推荐”-->
            <%if(rest.Expert && rest.Expert.ImgUrl && rest.Expert.Introduction && rest.Expert.RecommendedReason){%>
            <div class="mslls">
                <div class="mslls_intro">
                    <p class="ls"><%=rest.Expert.Name%>，<%=rest.Expert.Identity %></p>
                    <p class="zhicheng"><%=rest.Expert.Introduction%></p>
                </div>
                <span>
                    <img src="<%=rest.Expert.ImgUrl%>"/>
                </span>
            </div>
            <div class="travelrecmd">
                <p class="travelrecmd_comment"><%=rest.Expert.RecommendedReason%></p>
            </div>
            <%}else if(rest.RestaurantInfo.Feature){ %>
            <div class="travelrecmd nonmslrec">
                <p class="travelrecmd_comment"><span class="travelrecmd_nonmsl">旅行者推荐：<%=rest.RestaurantInfo.Feature%></span></p>
            </div>
            <% } %>
            <!-- 导航和电话-->
            <div class="nav">
                <p class="navtext <%if(Lizard.P('fromType') !== 'external'){%>js_address<%}%>" id="c_address_top"><%= rest.RestaurantInfo.Address %></p>
                <span class="navicons"><% if(commonUtil.isHybridMode() && Lizard.P('fromHotel')!='hotelFoodsDetail' && Lizard.P('fromType')!='hotel'){%><span class="js_navigation" id="c_guide_top">导航</span><%}%><%if(rest.RestaurantInfo.BookTelList && rest.RestaurantInfo.BookTelList.length){%><span class="js_jump_telpop" id="c_tel_top">电话</span><%}%></span>
            </div>
        </div>

        <!--推荐菜-->
        <%if(rest.RestaurantInfo.RestaurantRecommendedFoodList && rest.RestaurantInfo.RestaurantRecommendedFoodList.length > 0){ %>
        <div class="recomdish">
            <div  data-restid="<%=rest.RestaurantInfo.RestaurantId %>" class="title-detail">推荐菜</div>
            <div class="remdishlist">
                <% _.each(rest.RestaurantInfo.Foods,function(value,index){ %>
                <%= value %><% if(index != rest.RestaurantInfo.Foods.length -1){ %>&emsp;<% } %><% }) %>
            </div>
            <div class="swiper-box"></div>
            <% if(rest.Menus && rest.Menus.length>0){ %>
            <div class="seemenu js_allMenu" id="c_recommend_all_food" data-restid="<%= rest.RestaurantInfo.RestaurantId%>">查看全部菜单</div>
            <% } %>
        </div>
        <%}else if(rest.RestaurantInfo.Foods && rest.RestaurantInfo.Foods.length>0){%>
        <div class="recomdish">
            <div class="title-detail">推荐菜</div>
            <div class="remdishlist">
                <% rest.RestaurantInfo.Foods.some(function(v,i){
                if(i >= 10)return false; %><%= v %><% if( (i< rest.RestaurantInfo.Foods.length-1) && ( i < 9 )){ %>&emsp;<% }});%>
            </div>

        </div>
        <% } %>

        <!-- ↓↓ 订座·套餐 ↓↓ -->
        <% if(rest.ProductList && rest.ProductList.length>0 && Lizard.P('fromType') !== 'external'){%>
        <div class="bookingpackage">
            <%if(rest.hasBook){%>
            <div class="title-detail">订座·特惠</div>
            <%}else{%>
            <div class="title-detail">特惠·套餐</div>
            <%}%>
            <% _.each(rest.ProductList,function(product,index){%>
            <%if(index<3){%>
            <div class="c-product js_packmodules_series" id="c_entertainment_hui_<%=index%>" data-sourcetype="<%=product.SourceType %>" data-hybridurl="<%=product.CommonField1%>" data-url="<%=product.LinkUrl%>" <%if(product.SourceType == 'DiningCity'){%>data-dnid="<%=product.Id%>"<%}%> <%if(product.SourceType == 'Koubei'){%>data-id="<%='I'+ product.Id%>" data-subid="<%='I'+ product.SubId%>"<%}%>>
            <div class="c-product__item">
                <div class="c-product__title <%if(product.SourceType == 'Opentable'){%>opentable_style<%}%>">
                    <%=product.Title%>
                    <%if(product.SourceType == 'Opentable'){%>
                    <span>(<% if(rest.RestaurantInfo.BookingCount<=999){ %><%= rest.RestaurantInfo.BookingCount%><%}else{%>999+<%}%>人订过)</span>
                    <%}%>
                </div>
                <%if(product.SourceType == 'Koubei'){%>
                <div class="c-product__btag"><i>口碑优惠券</i></div>
                <%}%>
                <%if(product.SourceType == 'BankCard'){%>
                <div class="c-product__btag"><i>携程白金卡特惠</i></div>
                <%}%>

                <%if(product.SourceType !== 'Eztable' && product.SourceType !== 'Opentable'){%>
                <% if(product.CommentScore || product.SalesVolume){%>
                <div class="c-product__notice"><%if(product.CommentScore>0){%><span class="c-product__notice--score"><%=product.CommentScore%>分</span><%}%><%if(product.SalesVolume){%><span class="c-product__notice--text"><%=product.SalesVolume%></span><%}%></div>
                <%}else if(product.SubTitle){%>
                <div class="c-product__notice">
                    <span class="c-product__notice--text"><%=product.SubTitle %></span>
                </div>
                <%} %>
                <%}else if(product.SourceType == 'Eztable'){%>
                <div class="c-product__notice"><%if(rest.RestaurantInfo.BookingCount>0){%><span class="c-product__notice--text js_bookingnumber"><% if(rest.RestaurantInfo.BookingCount<=999){ %><%= rest.RestaurantInfo.BookingCount%><%}else{%>999+<%}%>人订过</span><%}%></div>
                <%}else if(product.SourceType == 'Opentable'){%>
                <div class="c-product__notice"><span class="c-product__notice--text">由opentable提供</span></div>
                <%}%>
            </div>
            <div class="c-product__booking">
                <%if(product.SourceType !== 'Eztable'){%>
                <%if(product.SourceType !== 'Koubei'&& product.SourceType !== 'BankCard' && product.Price){%>
                <div class="c-product__price">&yen;<em><%=product.Price%></em></div>
                <%}%>
                <%if(product.SourceType == 'Koubei'){ %>
                <div class="booking_button">领取</div>
                <% }else if(product.SourceType == 'BankCard'){ %>
                <div class="booking_button">查看</div>
                <%}else if(product.SourceType == 'DiningCity' || product.SourceType == 'Opentable'){%>
                <div class="booking_button">预约</div>
                <%}else{%>
                <div class="booking_button">预订</div>
                <%}%>
                <%}else{%>
                <div class="booking_button" data-linkurl="<%=rest.RestaurantInfo.LinkUrl%>" id="delicacies_detail_booking">预约</div>
                <%}%>
            </div>
        </div>
        <%}%>
        <% });%>
        <div class="js_packhidepart" style="height: 0;overflow: hidden;">
            <% _.each(rest.ProductList,function(product,index){%>
            <%if(index>2){%>
            <div class="c-product js_itemPack js_packmodules_series" id="c_entertainment_hui_<%=index%>" data-sourcetype="<%=product.SourceType %>" data-hybridurl="<%=product.CommonField1%>" data-url="<%=product.LinkUrl%>" <%if(product.SourceType == 'DiningCity'){%>data-dnid="<%=product.Id%>"<%}%> <%if(product.SourceType == 'Koubei'){%>data-id="<%='I'+product.Id%>" data-subid="<%='I'+product.SubId%>"<%}%>>
            <div class="c-product__item">
                <div class="c-product__title">
                    <%=product.Title%>
                </div>
                <%if(product.SourceType == 'Koubei'){%>
                <div class="c-product__btag"><i>口碑优惠券</i></div>
                <%}%>
                <%if(product.SourceType == 'BankCard'){%>
                <div class="c-product__btag"><i>携程白金卡特惠</i></div>
                <%}%>
                <% if(product.CommentScore || product.SalesVolume ){%>
                <div class="c-product__notice"><%if(product.CommentScore>0){%><span class="c-product__notice--score"><%=product.CommentScore%>分</span><%}%><%if(product.SalesVolume){%><span class="c-product__notice--text"><%=product.SalesVolume%></span><%}%></div>
                <%}else if(product.SubTitle){%>
                <div class="c-product__notice">
                    <span class="c-product__notice--text"><%=product.SubTitle %></span>
                </div>
                <%}%>
            </div>
            <div class="c-product__booking">
                <%if(product.SourceType !== 'Koubei' && product.SourceType !== 'BankCard' && product.Price){%>
                <div class="c-product__price">&yen;<em><%=product.Price%></em></div>
                <%}%>
                <%if(product.SourceType == 'Koubei'){ %>
                <div class="booking_button">领取</div>
                <% }else if(product.SourceType == 'BankCard'){ %>
                <div class="booking_button">查看</div>
                <%}else if(product.SourceType == 'DiningCity'){%>
                <div class="booking_button">预约</div>
                <%}else{%>
                <div class="booking_button">预订</div>
                <%}%>
